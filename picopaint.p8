function _init()
	menuitem(1, "-copy image", function() printh(pix, "@clip") end)
	menuitem(2, "-paste clipboard", clipcheck)
	menuitem(3, "-new canvas", function() pix = clear end)
	menuitem(4, "-move delay", toggledelay)
	menuitem(5, "-toggle debug", toggledebug)
	last_copy = stat(4)
end
   
function _update()
	cls()
	color(col)
	frames += 1
	drawframes += 1
	fontcol()
	px, py = x/‚ñí+1,y/‚ñí+1
	pos = py * 8 + px - 8
	hex = tostr(col,true)
	for x=0,7 do
--	idk how to 1 line this
		rectfill(x*‚ñí+1,0*‚ñí+1,x*‚ñí+14,0*‚ñí+14,tonum(pix[10+x],0x1))
		rectfill(x*‚ñí+1,1*‚ñí+1,x*‚ñí+14,1*‚ñí+14,tonum(pix[18+x],0x1))
		rectfill(x*‚ñí+1,2*‚ñí+1,x*‚ñí+14,2*‚ñí+14,tonum(pix[26+x],0x1))
		rectfill(x*‚ñí+1,3*‚ñí+1,x*‚ñí+14,3*‚ñí+14,tonum(pix[34+x],0x1))
		rectfill(x*‚ñí+1,4*‚ñí+1,x*‚ñí+14,4*‚ñí+14,tonum(pix[42+x],0x1))
		rectfill(x*‚ñí+1,5*‚ñí+1,x*‚ñí+14,5*‚ñí+14,tonum(pix[50+x],0x1))
		rectfill(x*‚ñí+1,6*‚ñí+1,x*‚ñí+14,6*‚ñí+14,tonum(pix[58+x],0x1))
		rectfill(x*‚ñí+1,7*‚ñí+1,x*‚ñí+14,7*‚ñí+14,tonum(pix[66+x],0x1))
		end
	if hidedebug then
		rectfill(x+1,y+1,x+14,y+14,col)
		?"p="..hex[6],x+2,y+2,fcol
		?"d="..delay,x+2,y+9,fcol
	else
		if col == 0 and pix[pos+9] != tostr(0) then
			rect(x,y,x+15,y+15,pix[pos+9])
		elseif col == 0 and pix[pos+9] == tostr(0) then
			rect(x,y,x+15,y+15,7)
		else
			rect(x,y,x+15,y+15,tonum(pix[pos+9],0x1))
		end
		rectfill(x+2,y+2,x+13,y+13,col)
	end
	if not hidedebug then
--		?"pix:"..pix,dcol
--		?"cpy:"..stat(4),dcol
--		?"lst:"..last_copy,dcol
		?"px,py,pos:"..px..","..py..","..pos,dcol
		?"delay,fg pal,bg pal:"..delay..","..hex[6]..","..pix[pos+9],dcol
	end
	if btn(‚ùé) then
		if drawframes >= delay then
			drawframes = 0
			pix = "[gfx]0808"..setchar(sub(pix,10,73),pos,hex[6]).."[/gfx]"
		end
	end
	if btn(üÖæÔ∏è) then
		if frames >= delay then
			frames = 0
			if btn(‚¨ÜÔ∏è) then
				if col < 15 then
					col += 1
				elseif col == 15 then
					col = 0
				end
			elseif btn(‚¨áÔ∏è) then
				if col > 0 then
					col -= 1
				elseif col == 0 then
					col = 15
				end
			elseif btn(‚¨ÖÔ∏è) then
				col = tonum(pix[pos+9],0x1)
			elseif btn(‚û°Ô∏è) then
				if not hidedebug then
					debugcol()
				else
					toggledelay()
				end
			end
		end
	end
	if not btn(üÖæÔ∏è) then
		if frames >= delay then
			frames = 0
			if btn(‚¨ÜÔ∏è) then
				if y > 0 then
					y -= 1 * ‚ñí
				else
					y = 7 * ‚ñí
				end
			elseif btn(‚¨áÔ∏è) then
				if y < 112 then
					y += 1 * ‚ñí
				else
					y = 0
				end
			elseif btn(‚¨ÖÔ∏è) then
				if x > 0 then
					x -= 1 * ‚ñí
				else
					x = 7 * ‚ñí
				end
			elseif btn(‚û°Ô∏è) then
				if x < 112 then
					x += 1 * ‚ñí
				else
					x = 0
				end
			end
		end
	end
end

function debugcol()
	if dcol < 15 then
		dcol += 1
	else
		dcol = 0
	end
end

function fontcol()
	if col == 0 then
		fcol = 7
	else
		fcol = 0
	end
end

function clipcheck()
	if #stat(4) == 79 and sub(stat(4),1,9) == "[gfx]0808" and sub(stat(4),74,79) == "[/gfx]" then
		pix = stat(4)
		last_copy = stat(4)
	end
end

function toggledelay()
	if delay < 4 then
		delay+=1
	else
		delay = 2
	end
end
	
function toggledebug()
 hidedebug = not hidedebug
end

function setchar(s,i,c)
 return sub(s,1,i-1)..c..sub(s,i+1)
end
